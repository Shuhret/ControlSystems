                        <Grid>
                            <Border>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Width="12" Height="12" BorderThickness="1" BorderBrush="{TemplateBinding BorderBrush}" Margin="0,0,0,1">
